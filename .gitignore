# ==============================================
# Laravel .gitignore
# ==============================================

# Laravel
/vendor/
/node_modules/
/public/build/
/public/hot
/public/storage
/storage/*.key
.env
.env.backup
.env.production
.phpunit.result.cache
Homestead.json
Homestead.yaml
auth.json
npm-debug.log
yarn-error.log

# IDE
/.idea
/.vscode
*.swp
*.swo
.DS_Store

# Logs
*.log
/storage/logs/*
!/storage/logs/.gitkeep

# Cache
/bootstrap/cache/*
!/bootstrap/cache/.gitkeep

# Compiled files
*.compiled.php

# Sitemaps (generated at runtime)
/public/sitemap*.xml

# Docker volumes data
/data/
/logs/

# Testing
/coverage
/.phpunit.cache

# OS
Thumbs.db
